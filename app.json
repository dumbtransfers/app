{
  "expo": {
    "name": "dumbtransfers-app",
    "scheme": "wapu",
    "slug": "wapu",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "infoPlist": {
        "LSApplicationQueriesSchemes": [
          "metamask",
          "trust",
          "safe",
          "rainbow",
          "uniswap"
        ],
        "UIBackgroundModes": ["remote-notification"]
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.lausuarez02.dumbtransfers-app",
      "runtimeVersion": "1.0.0",
      "buildNumber": "1.0.1",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "runtimeVersion": {
        "policy": "appVersion"
      },
      "package": "com.lausuarez02.umbtransfersapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
    
      "eas": {
        "projectId": "2f33e73a-89f9-4695-be4d-6e60835021de"
      }
    },
    "owner": "lausuarez02"
  }
}
